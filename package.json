    "lint:style:fix": "stylelint --fix './**/*.module.css'",
    "postinstall": "(command -v husky && husky install && syncpack format) || echo 'husky is not installed, skipping'; test -f .env.local || (test -f .env.sample && cp .env.sample .env.local || echo -e \"\\e[33mNo env file initialized\\e[0m\")",

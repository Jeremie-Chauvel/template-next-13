    "prepare": "(command -v husky && husky install ) || echo 'husky is not installed, skipping'",
    "security": "audit-ci --config ./audit-ci.json",
    "start": "next start",
    "test": "jest",
    "test:circular": "pnpm depcruise --validate .dependency-cruiser.js pages --ignore-known",
    "test:coverage": "pnpm test -- --coverage",
    "test:type": "pnpm build"
  },
